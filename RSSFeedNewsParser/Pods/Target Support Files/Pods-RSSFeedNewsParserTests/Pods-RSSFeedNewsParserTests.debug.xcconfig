FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/Fuzi"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/Fuzi/Fuzi.framework/Headers" $(SDKROOT)/usr/include/libxml2
OTHER_LDFLAGS = $(inherited) -l"xml2" -framework "Fuzi"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
SWIFT_INCLUDE_PATHS = $(inherited) $(SRCROOT)/Fuzi/libxml2
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
